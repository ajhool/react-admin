{
  "version": "0.0.1",
  "compilerOptions": {
      "declaration": true,
      "lib": ["es6", "dom"],
      "module": "commonjs",
      "noFallthroughCasesInSwitch": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "sourceMap": false,
      "strict": true,
      "target": "es5",
      "jsx": "react"
  }
}